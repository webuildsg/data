{
  "name": "data.webuild.sg",
  "version": "1.0.0",
  "description": "using data and graphs to inspire variety, openness and projects for the design and engineering community in Singapore",
  "keywords": [
    "archives",
    "data",
    "snapshot",
    "events",
    "meetups",
    "repos",
    "repositories",
    "webuild.sg",
    "developer",
    "engineer",
    "designer",
    "open source",
    "graphs",
    "charts"
  ],
  "homepage": "https://github.com/webuildsg/data#readme",
  "bugs": {
    "url": "https://github.com/webuildsg/data/issues"
  },
  "license": "MIT",
  "main": "index.html",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webuildsg/data.git"
  },
  "private": true,
  "scripts": {
    "build:html": "gulp",
    "build:graph": "node graphs",
    "build": "npm run build:html && npm run build:graph",
    "check": "node tasks/check",
    "clean": "rm index.html && rm -rf dataset  && rm -rf public/data",
    "deploy": "git push origin gh-pages",
    "start": "http-server ./",
    "status": "notes graphs tasks test views && npm outdated",
    "test": "jshint graphs tasks test public/js && NODE_ENV=test mocha test --recursive && jsinspect ./tasks ./graphs -t 30 -i"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-jade": "^1.1.0",
    "http-server": "^0.8.5",
    "jade": "^1.11.0",
    "jshint": "^2.8.0",
    "jsinspect": "^0.7.2",
    "mocha": "^2.3.3",
    "notes": "0.0.4"
  },
  "dependencies": {
    "geocoder": "^0.2.2",
    "geojson": "^0.2.1",
    "lodash": "^3.10.1",
    "moment-timezone": "^0.4.1",
    "word-overlap": "^2.4.1"
  }
}
