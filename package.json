{
  "name": "data.webuild.sg",
  "version": "1.0.0",
  "description": "store snapshot data for webuild.sg for events and repos",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "private": true,
  "scripts": {
    "build": "jade views/index.jade -o . -P && node graphs/reposPerProgramming.js && node graphs/eventsPerGroup.js && node graphs/reposPerWeek.js && node graphs/eventsPerWeek.js",
    "clean": "rm index.html",
    "status": "notes && npm outdated",
    "check-data": "node tasks/check",
    "test": "jshint graphs tasks test && NODE_ENV=test mocha",
    "start": "http-server ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webuildsg/data.git"
  },
  "keywords": [
    "archives",
    "data",
    "events",
    "repos",
    "webuild.sg",
    "developer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webuildsg/data/issues"
  },
  "homepage": "https://github.com/webuildsg/data#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "http-server": "^0.8.5",
    "jade": "^1.11.0",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "notes": "0.0.4"
  },
  "dependencies": {
    "moment-timezone": "^0.4.1"
  }
}
